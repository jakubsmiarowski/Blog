{
    "name": "MERN",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "dependencies": {
        "heroku-cli": "^7.0.9",
        "npm-run-all": "4.1.5",
        "react-redux": "7.0.1",
        "react-scripts": "^3.1.2",
        "redux": "4.0.1"
    },
    "scripts": {
        "start": "npm-run-all -p -l start:*",
        "start:server": "cd server && yarn start",
        "start:client": "cd client && yarn start",
        "install:client": "cd client && npm install",
        "install:server": "cd server && npm install",
        "postinstall": "run-p install:*"
    }
}
